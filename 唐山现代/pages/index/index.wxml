<!--index.wxml-->
<view class="container">
  <image src="../../images/banner.png"  class="img">  </image>

  <block wx:for="{{datalist}}" wx:key="allen">
    <view class="listitem" catchtap="cellClick" data-id="{{item.id}}">
        <image src="{{item.img}}"></image>
        <view class="subview">
           <view class="subview-item" wx:for="{{item.title}}" wx:for-item="listitem" wx:key="allenzhan">
              <text style="font-size:10pt" >{{listitem}}</text>
           </view>
        </view>
    </view>
  </block>

  <view class="" style="margin-top:8px;">现代男科:让看病成为一种享受</view>
  <view class="foot">
      <image src="../../images/index1.png"></image>
      <image src="../../images/index2.png"></image>
      <image src="../../images/index3.png"></image>
      <image src="../../images/index4.png"></image>
  </view>



</view>
