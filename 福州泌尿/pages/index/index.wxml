<!--index.wxml-->


<view class="container" >
<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="true" style="width:100%;background:red">
  <block wx:for="{{imgUrls}}" wx:key="swiper">
    <swiper-item style="height:150px;width:100%;background:blue;">
    <view class="" style="width:100%;height:100%">
       <image src="{{item}}" style=" width:100%; "></image>  
    </view>
    </swiper-item>
  </block>
</swiper>


 

<block wx:for="{{dataList}}" wx:key="hehhe">
  <view class="bgView"  catchtap="cellClick" data-id="{{item.id}}" >
    <template is="cell" data="{{...item}}"  ></template>
  </view>
</block>


<view class="" style="height:10px;width:100%"></view>
<image src="../../images/zhyb_01.jpg" mode="widthFix" style="width:100%"></image>
<image src="../../images/zhyb_02.jpg" mode="widthFix" style="width:100%"></image>
<image src="../../images/zhyb_03.jpg" mode="widthFix" style="width:100%"></image>
<image src="../../images/zhyb_04.jpg" mode="widthFix" style="width:100%"></image>


<!--使用模板-->
<template name="cell" >
  <view class="" style="margin-top:8px;"> <text style="color:{{color}};text-align:center" >{{title}}</text> </view>
   <view class="subview">
       <view class="subview-item" wx:for="{{detial}}" wx:for-item="listitem" wx:key="allenzhan" >
          <text style="font-size:10pt" >{{listitem}}</text>
       </view>
   </view>
</template>


</view>
