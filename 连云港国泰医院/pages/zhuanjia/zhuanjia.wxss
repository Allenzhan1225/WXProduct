.bg{
  display: flex;

  width: 94%;
  padding: 3%;
}

.cell{
  display: flex;
  flex-direction: column;
  /*flex-direction: row;*/
  /*flex-wrap: wrap;*/
  width: 94%;

}

.cell-item{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 130px;
  background-color: blue;
  
  
}


.container{
  display: flex;  
  padding: 0px;
  width: 100%;

}


.listitem{
  display: flex;
  flex-direction: row;
  align-self: flex-start;
  margin: 8px;


}


.bgView{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}


.subview{

  display: flex;
  flex: 1;
  flex-direction: row;
  flex-wrap: wrap; 
  width: 100%;
  margin-top:8px;
  
}

.subview-item{
  display: flex;
  flex-direction: column;
  width: 30%;
  /* height: 44px; */
  margin-top: 4px;
  margin-left: 8px;
  font-size: 10pt;
  border: 1px solid lightgray;
  align-items: center;
  justify-content: center;
  height: 150px;
}


.foot{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 8px;
  width: 100%;

}

.foot image {
  height: 200px;
  width: 50%;

}